<animation>
  <type>translate</type>
  <object-name>button</object-name>
  <property>/systems/horn</property>
  <factor>0.001</factor>
  <axis>
    <x>-1.0</x>
    <y>0.0</y>
    <z>0.0</z>
  </axis>
</animation>

<!--Control-->
<animation>
  <type>pick</type>
  <object-name>button</object-name>
  <visible>true</visible>
  <action>
        <button>0</button>
        <repeatable>false</repeatable>
      <binding>
        <command>property-assign</command>
        <property>sim/multiplay/generic/int[2]</property>
        <value>1</value>
      </binding>
    <mod-up>
        <binding>
        <command>property-assign</command>
        <property>sim/multiplay/generic/int[2]</property>
        <value>0</value>
        </binding>
    </mod-up>
  </action>
</animation>


</PropertyList>
